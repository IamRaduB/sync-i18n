{
  "name": "@iamradub/sync-locales",
  "version": "0.0.1",
  "description": "Command line tool that allows you to sync translation files",
  "main": "dist/src/index.js",
  "bin": {
    "sync-i18n": "dist/src/index.js"
  },
  "scripts": {
    "build": "tsc -b",
    "validate-all": "npm run build && node dist/src/index.js validate ./i18n en.json",
    "validate-some": "npm run build && node dist/src/index.js validate ./i18n en.json ro.json",
    "test": "ts-mocha src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IamRaduB/sync-locales.git"
  },
  "author": "IamRaduB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IamRaduB/sync-locales/issues"
  },
  "homepage": "https://github.com/IamRaduB/sync-locales#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/chai": "^4.2.14",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.2",
    "@types/sinon": "^9.0.8",
    "mocha": "^8.2.1",
    "sinon": "^9.2.1",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.5"
  }
}
